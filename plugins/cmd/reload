		$networks[network + "_pie"] = 0
        $networks[network + "_reloaded"] = 1
        load Dir.pwd + "/depends/reqfunc.rb"
        load Dir.pwd + "/plugins/func.rb"
		load Dir.pwd + "/plugins/libs.rb"
        load_plugins()
        load_users(network)
		load_settings()
		load Dir.pwd + "/depends/cmd.rb"
		load Dir.pwd + "/plugins/message.rb"
		puts "Reloaded.. kinda?"